        $(function () {
            var topBar = $("#languageRedirectOption");
            topBar.on("click", ".close", function () {
                var wayOffFutureDate = new Date();
                wayOffFutureDate.setTime(wayOffFutureDate.getTime() + (1000 * 24 * 60 * 60 * 1000));
                document.cookie = "IAmHappyWhereIAm=IGotSunshine;expires=" + wayOffFutureDate.toGMTString();
                topBar.remove();
            });
            setTimeout(function () {
                topBar.slideDown("slow");
            }, 1500);
        });
    </script>


        

